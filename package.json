    "@commercetools/sdk-middleware-auth": "^7.0.1",
    "@commercetools/sdk-middleware-http": "^7.0.4",
    "@commercetools/typescript-sdk": "^0.0.22",
    "node-fetch": "^3.3.2",
    "ts-dotenv": "^0.9.1"
  }
